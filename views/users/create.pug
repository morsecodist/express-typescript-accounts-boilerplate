extends ../layout

block content
  h1 Create Account
  a(href="/") Home
  form(action='/users/create' method='POST')
    p Username:
      input(type='text', name='username', value=valueID)
    p Email:
      input(type='text', name='email', value=valueID)
    p Password:
      input(type='password', name='password', value=valueID)
    p
      input(type='submit' value='Submit')
